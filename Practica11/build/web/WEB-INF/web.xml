<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>InsertContact</servlet-name>
        <servlet-class>web.InsertContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteContact</servlet-name>
        <servlet-class>web.DeleteContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateContact</servlet-name>
        <servlet-class>web.UpdateContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SelectContact</servlet-name>
        <servlet-class>web.SelectContact</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertContact</servlet-name>
        <url-pattern>/InsertContact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteContact</servlet-name>
        <url-pattern>/DeleteContact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateContact</servlet-name>
        <url-pattern>/UpdateContact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SelectContact</servlet-name>
        <url-pattern>/SelectContact</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
